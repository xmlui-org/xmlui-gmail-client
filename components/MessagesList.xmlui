<Component name="MessagesList">
  <VStack gap="space-4" width="100%" height="100%">
    <H3 marginLeft="20px">
      Messages ({ $props.messagesList.length })
    </H3>
    <Items data="{ $props.messagesList }">
      <MessageCard
        messageId="{ $item.id }"
        accessToken="{ $props.auth.tokens.access_token }"
      />
    </Items>
  </VStack>
</Component>